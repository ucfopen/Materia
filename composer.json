{
    "name": "ucfcdl/materia",
    "type": "project",
    "description" : "Materia is UCF's educational game and application platform",
    "keywords": ["materia", "ucf", "cdl", "games", "edutainment", "widgets", "education", "university of central florida"],
    "license": "No License",
    "require": {
        "php": ">=5.6.0",
        "composer/installers": "~1.0",
        "fuel/core": "1.8.*",
        "fuel/auth": "1.8.*",
        "fuel/email": "1.8.*",
        "fuel/oil": "1.8.*",
        "fuel/orm": "1.8.*",
        "fuel/parser": "1.8.*",
        "iturgeon/qasset": "1.0.4",
        "fuelphp/upload": "2.0.6",
        "monolog/monolog": "1.18.*",
        "phpseclib/phpseclib": "2.0.0",
        "eher/oauth": "1.0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "suggest": {
        "fuel/docs": "FuelPHP documentation"
    },
    "config": {
        "vendor-dir": "fuel/vendor",
        "optimize-autoloader": true
    },
    "extra": {
        "installer-paths": {
            "fuel/{$name}": ["fuel/core"],
            "{$name}": ["fuel/docs"]
        }
    },
    "minimum-stability": "dev"
}
